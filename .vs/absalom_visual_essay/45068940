});\n  }, options);\n  observer.observe(video);\n\n  const $main = $(\u0022#container\u0022);\n  const $scrolly = $main.find(\u0022#scrolly\u0022);\n  const $figure = $scrolly.find(\u0022figure\u0022);\n  const $article = $scrolly.find(\u0022article\u0022);\n  const $step = $article.find(\u0022.step\u0022);\n\n  const resizeStepElements = () =\u003E {\n    const stepH = Math.floor(window.innerHeight * 0.75);\n    $step.css(\u0022height\u0022, \u0060${stepH}px\u0060);\n  }\n\n  const resizeFigureElement = () =\u003E {\n    const figureHeight = window.innerHeight / 2;\n    const figureMarginTop = (window.innerHeight - figureHeight) / 2;\n\n    $figure.css(\u0022height\u0022, \u0060${figureHeight}px\u0060)\n          .css(\u0022top\u0022, \u0060${figureMarginTop}px\u0060);\n  }\n\n  const handleResize = () =\u003E {\n    resizeStepElements();\n    resizeFigureElement();\n  }\n  \n  const handleStepEnter = (response) =\u003E {\n\n    $step.removeClass(\u0022is-active\u0022);\n    $step.eq(response.index).addClass(\u0022is-active\u0022);\n\n    startAnimation(response.index);\n  }\n\n  const scroller = scrollama().setup({\n    step: \u0022#scrolly article .step\u0022,\n    offset: 0.33,\n    debug: false\n  }).onStepEnter(handleStepEnter);\n\n  const debounce = (func, wait, immediate) =\u003E {\n    let timeout;\n    return () =\u003E {\n      const context = this, args = arguments;\n      const later = () =\u003E {\n        timeout = null;\n        if (!immediate) func.apply(context, args);\n      };\n      const callNow = immediate \u0026\u0026 !timeout;\n      clearTimeout(timeout);\n      timeout = setTimeout(later, wait);\n      if (callNow) func.apply(context, args);\n    };\n  }\n  \n  const handleResizeDebounced = debounce(handleResize, 100);\n  window.addEventListener(\u0022resize\u0022, handleResizeDebounced);\n  \n  resizeStepElements();\n  resizeFigureElement();\n}\n\nconst startAnimation = (index) =\u003E {\n  const $currentStep = $step.filter((d, i) =\u003E i === index);\n  const $currentImg = $currentStep.find(\u0022img\u0022);\n\n  $currentImg.css(\u0022opacity\u0022, 0);\n\n  window.requestAnimationFrame(() =\u003E {\n    $currentImg.css(\u0022opacity\u0022, 1);\n  });\n}\n\n$(document).ready(() =\u003E init());\n\u0060\u0060\u0060","Type":1,"Status":1,"FollowUpPrompts":null},"SelectedFunctionName":"Generate","LoggedSkillInvocations":null}
